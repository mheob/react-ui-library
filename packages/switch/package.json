{
  "name": "@mheob/switch",
  "version": "0.0.0",
  "description": "A React component to view and switch between on or off states.",
  "keywords": [
    "easyfeedback",
    "switch",
    "toggle",
    "component",
    "input",
    "react",
    "custom checkbox"
  ],
  "homepage": "https://github.com/mheob/react-ui-library#readme",
  "bugs": {
    "url": "https://github.com/mheob/react-ui-library/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mheob/react-ui-library.git"
  },
  "license": "MIT",
  "author": "Alexander BÃ¶hm",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "concurrently yarn:build:*",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --root-mode upward --extensions .ts,.tsx -d dist/cjs --source-maps",
    "build:esm": "cross-env BABEL_ENV=esm babel src --root-mode upward --extensions .ts,.tsx -d dist/esm --source-maps",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "lint": "concurrently yarn:lint:*",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path ../../config/.eslintignore",
    "lint:style": "stylelint '**/*.{css,scss,js,jsx,ts,tsx}' --ignore-path ../../config/.stylelintignore",
    "lint:types": "tsc --noEmit",
    "test": "jest --env=jsdom --passWithNoTests",
    "test:ci": "jest --runInBand",
    "test:ci:cov": "yarn test:ci --coverage",
    "test:cov": "yarn test --coverage",
    "test:watch": "yarn test --watch -u"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.4",
    "react": "^17.0.2"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.0",
    "react": "^16.13.0 || ^17.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
