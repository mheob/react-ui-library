{
  "name": "@mheob/test-utils",
  "version": "0.0.0",
  "private": true,
  "description": "Testing utilities for the components.",
  "homepage": "https://github.com/mheob/react-ui-library#readme",
  "bugs": {
    "url": "https://github.com/mheob/react-ui-library/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mheob/react-ui-library.git"
  },
  "license": "MIT",
  "author": "Alexander BÃ¶hm",
  "main": "src/index.ts",
  "scripts": {
    "lint": "concurrently yarn:lint:*",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path ../../config/.eslintignore",
    "lint:style": "stylelint '**/*.{css,scss,js,jsx,ts,tsx}' --ignore-path ../../config/.stylelintignore",
    "lint:types": "tsc --noEmit",
    "test": "jest --env=jsdom --passWithNoTests --maxWorkers=50%",
    "test:ci": "jest --runInBand",
    "test:ci:cov": "yarn test:ci --coverage",
    "test:cov": "yarn test --coverage",
    "test:watch": "yarn test --watch -u"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.2",
    "@testing-library/user-event": "^13.1.8",
    "@types/jest-axe": "^3.5.1",
    "jest-axe": "^4.1.0",
    "react-test-renderer": "^17.0.2"
  },
  "devDependencies": {
    "react": "^17.0.2"
  },
  "peerDependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "react": ">=16.13.0 <18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
